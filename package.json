{
    "name": "lemonyte.github.io",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
    },
    "devDependencies": {
        "@sveltejs/adapter-static": "^3.0.8",
        "@sveltejs/kit": "^2.15.1",
        "@sveltejs/vite-plugin-svelte": "^5.0.3",
        "autoprefixer": "^10.4.20",
        "postcss": "^8.4.49",
        "svelte": "^5.16.0",
        "svelte-check": "^4.1.1",
        "tailwindcss": "^3.4.17",
        "tslib": "^2.8.1",
        "typescript": "^5.7.2",
        "vite": "^6.0.6"
    },
    "prettier": {
        "tabWidth": 4,
        "printWidth": 119,
        "trailingComma": "all",
        "endOfLine": "crlf",
        "overrides": [
            {
                "files": [
                    "*.yaml",
                    "*.yml"
                ],
                "options": {
                    "tabWidth": 2
                }
            }
        ]
    },
    "type": "module"
}
