{
    "name": "lemonyte.github.io",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
    },
    "devDependencies": {
        "@sveltejs/adapter-static": "^2.0.3",
        "@sveltejs/kit": "^1.27.7",
        "autoprefixer": "^10.4.16",
        "postcss": "^8.4.31",
        "svelte": "^4.2.1",
        "svelte-check": "^3.5.2",
        "tailwindcss": "^3.3.3",
        "tslib": "^2.6.2",
        "typescript": "^5.2.2",
        "vite": "^4.4.12"
    },
    "prettier": {
        "tabWidth": 4,
        "printWidth": 119,
        "trailingComma": "all",
        "endOfLine": "crlf",
        "overrides": [
            {
                "files": [
                    "*.yaml",
                    "*.yml"
                ],
                "options": {
                    "tabWidth": 2
                }
            }
        ]
    },
    "type": "module"
}
